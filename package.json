{
  "name": "postcode",
  "version": "2.0.1",
  "description": "UK Postcode helper methods",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {},
  "prettier": {},
  "devDependencies": {
    "@cablanchard/eslint-config": "~1.1.1",
    "@cablanchard/semantic-release": "~1.3.1",
    "@cablanchard/tsconfig": "1.0.0",
    "@cablanchard/tslint": "0.0.1",
    "@types/benchmark": "~1.0.31",
    "@types/chai": "~4.2.0",
    "@types/csv-parse": "~1.2.2",
    "@types/mocha": "~7.0.1",
    "@types/node": "~13.13.0",
    "benchmark": "~2.1.4",
    "chai": "~4.2.0",
    "codecov": "~3.6.1",
    "csv-parse": "~4.8.2",
    "eslint": "~6.8.0",
    "install-npm-version": "~1.0.3",
    "mocha": "~7.1.0",
    "nyc": "~15.0.0",
    "prettier": "~2.0.1",
    "semantic-release": "~15.14.0",
    "source-map-support": "~0.5.11",
    "ts-node": "~8.8.1",
    "tslint": "~5.20.0",
    "typedoc": "~0.17.0",
    "typescript": "~3.8.2"
  },
  "mocha": {
    "extension": [
      "js",
      "ts"
    ],
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "files": [
    "dist",
    "example.js"
  ],
  "runkitExampleFilename": "example.js",
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "generate/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts",
      "generate/index.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "scripts": {
    "semantic-release": "semantic-release --no-ci",
    "test": "npm run lint && NODE_ENV=test nyc mocha",
    "prepare": "npm test && npm run build",
    "lint": "eslint lib/**/*.ts",
    "docs": "typedoc --mode file --excludePrivate --hideGenerator --excludeExternals --target ES5 --out docs/ --name 'Postcode.js' --theme minimal lib/",
    "build": "tsc",
    "benchmark": "npm run build && node benchmark.js",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ideal-postcodes/postcode.git"
  },
  "keywords": [
    "uk",
    "postcode",
    "validation",
    "parsing"
  ],
  "author": {
    "name": "Ideal Postcodes",
    "url": "https://ideal-postcodes.co.uk",
    "email": "support@ideal-postcodes.co.uk"
  },
  "license": "MIT",
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm"
  },
  "bugs": {
    "url": "https://github.com/ideal-postcodes/postcode/issues"
  }
}
